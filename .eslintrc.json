{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "class-methods-use-this":"off",
        "no-underscore-dangle":"off",
        "no-unused-vars":"off",
        "camelcase":"off"
    }
}
